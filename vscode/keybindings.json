// Place your key bindings in this file to override the defaults
[
  {
      "key": "cmd+'",
      "command": "workbench.action.nextEditor"
  },
  {
      "key": "alt+cmd+right",
      "command": "-workbench.action.nextEditor"
  },
  {
      "key": "cmd+;",
      "command": "workbench.action.previousEditor"
  },
  {
      "key": "alt+cmd+left",
      "command": "-workbench.action.previousEditor"
  },
  {
      "key": "ctrl+u",
      "command": "cursorPageUp",
      "when": "textInputFocus"
  },
  {
      "key": "pageup",
      "command": "-cursorPageUp",
      "when": "textInputFocus"
  },
  {
      "key": "ctrl+d",
      "command": "cursorPageDown",
      "when": "textInputFocus"
  },
  {
      "key": "pagedown",
      "command": "-cursorPageDown",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+]",
      "command": "workbench.action.terminal.focusNextPane",
      "when": "terminalFocus"
  },
  {
      "key": "alt+cmd+down",
      "command": "-workbench.action.terminal.focusNextPane",
      "when": "terminalFocus"
  },
  {
      "key": "alt+cmd+right",
      "command": "-workbench.action.terminal.focusNextPane",
      "when": "terminalFocus"
  },
  {
      "key": "cmd+[",
      "command": "workbench.action.terminal.focusPreviousPane",
      "when": "terminalFocus"
  },
  {
      "key": "alt+cmd+up",
      "command": "-workbench.action.terminal.focusPreviousPane",
      "when": "terminalFocus"
  },
  {
      "key": "ctrl+s",
      "command": "search.action.openNewEditor"
  },
  {
      "key": "cmd+r",
      "command": "workbench.action.terminal.runSelectedText"
  },
  {
    "key": "alt+k",
    "command": "extension.selectDoubleQuote",
    "when": "editorFocus"
  },
  {
    "key": "cmd+k shift+'",
    "command": "-extension.selectDoubleQuote",
    "when": "editorFocus"
  },
  {
    "key": "alt+j",
    "command": "extension.selectSingleQuote",
    "when": "editorFocus"
  },
  {
    "key": "cmd+k '",
    "command": "-extension.selectSingleQuote",
    "when": "editorFocus"
  }
]